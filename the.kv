<initialscreen>

	canvas.before:
		Color:
			rgba: 1, 1, 1, 1
 
		Rectangle:
			size: root.size
			pos: 0, 0


	GridLayout:
		size: root.width*0.8, root.height*0.05
		pos: root.width*0.1, root.height*0.5
		cols: 2 
		spacing: 50

		Button:
			text: 'Start Service'
			on_press: root.start_service('Testservice')

		Button:
			text: 'Stop Service'
			on_press: root.stop_service('Testservice')



